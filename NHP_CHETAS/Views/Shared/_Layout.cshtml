    <!-- Page Scroll nav-tab start -->
    <script>
        $(window).scroll(function() {
            var sticky = $('#navbar'),
                scroll = $(window).scrollTop();

            if (scroll >= 495) {
                sticky.addClass('fixed');
                /*$("#navbar a.navB").show();*/

                $('#navbar a.navB').addClass("navbar-brand animated fadeInLeft");
                $("#navbar a.navB").css("color", "#ffffff");

            }
            else {
                sticky.removeClass('fixed');
                /*$("#navbar a.navB").css("color", "transparent");*/

            }
        });
    </script>
    <!-- Page Scroll nav-tab end -->

