    List<string> inflowDatalst = new List<string>();
    List<string> outflowDatalst = new List<string>();
    List<string> tagTbaleDatalst = new List<string>();
    //List<string> HROutflowDatalst = new List<string>();
    var inflowdataBind = "";
    var outflowdataBind = "";
    var tagTableflowdataBind = "";
    //var HROutflowdataBind = "";
    foreach (var item in Model.DamInflowTagTableForGraph)
    {
        inflowDatalst.Add(item.LstValue);
    }
    foreach (var item in Model.dam_outflow_tag_tableForGraph)
    {
        outflowDatalst.Add(item.LstValue);
    }
    foreach (var item in Model.TagTable)
    {
        tagTbaleDatalst.Add(item.LstValue);
    }

    //foreach (var item in Model.HrOutflowtagTableForGraph)
    //{
    //    tagTbaleDatalst.Add(item.LstValue);
    //}

    inflowdataBind = String.Join(";", inflowDatalst.ToArray());
    outflowdataBind = String.Join(";", outflowDatalst.ToArray());
    tagTableflowdataBind = String.Join(";", tagTbaleDatalst.ToArray());
    //HROutflowdataBind = string.Join(";", HROutflowDatalst.ToArray());
}
